msgstr "USB-enheten ikke oppdaget"

#: ../ui/dwindowui.cpp:114 ../ui/dwindowui.cpp:212 ../ui/dwindowui.cpp:218
#, qt-format
msgctxt "DWindowUI|"
msgid "<p style='color:white; font-size:12px;'>%1% has been completed</p>"
msgstr "<p style='color:white; font-size:12px;'>%1% har blitt fullført</p>"

#: ../ui/dwindowui.cpp:127
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#b4b4b4; font-size:12px;'>The making success rate can be "
"improved by formatting USB drive.</p>"
msgstr "<p style='color:#b4b4b4; font-size:12px;'>Suksessraten kan forbedres ved å formatere USB-enheten.</p>"

#: ../ui/dwindowui.cpp:138
msgctxt "DWindowUI|"
msgid "Please DO NOT remove the USB drive or shutdown while file is writing."
msgstr "Vennligst IKKE fjern USB-enheten eller slå av når filen skrives."

#: ../ui/dwindowui.cpp:149
msgctxt "DWindowUI|"
msgid "Start"
msgstr "Start"

#: ../ui/dwindowui.cpp:257
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#ffffff; font-weight:blod; font-size:18px'>Made "
"successfully</p>"
msgstr "<p style='color:#ffffff; font-weight:blod; font-size:18px'>Laget med suksess</p>"

#: ../ui/dwindowui.cpp:258
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#b4b4b4; font-size:12px'>Please select USB drive to boot "
"when installing Deepin OS.</p>"
msgstr "<p style='color:#b4b4b4; font-size:12px'>Velg USB-enheten for å starte opp når du installerer Deepin OS.</p>"

#: ../ui/dwindowui.cpp:262
msgctxt "DWindowUI|"
msgid ""
"<a style='color:#b4b4b4; font-size:12px'>If failed, please try to disable "
"EFI option.</a>"
msgstr "<a style='color:#b4b4b4; font-size:12px'>Hvis feilet, prøv å deaktivere EFI alternativet.</a>"

#: ../ui/dwindowui.cpp:280
