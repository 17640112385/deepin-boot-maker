#, qt-format
msgctxt "DWindowUI|"
msgid "<p style='color:white; font-size:12px;'>%1% has been completed</p>"
msgstr "<p style='color:white; font-size:12px;'>%1% foi completada</p>"

#: ../ui/dwindowui.cpp:127
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#b4b4b4; font-size:12px;'>The making success rate can be "
"improved by formatting USB drive.</p>"
msgstr "<p style='color:#b4b4b4; font-size:12px;'>A taxa de sucesso de criação pode ser melhorada formatando a unidade USB.</p>"

#: ../ui/dwindowui.cpp:138
msgctxt "DWindowUI|"
msgid "Please DO NOT remove the USB drive or shutdown while file is writing."
msgstr "Por favor NÃO remova ou desligue a entrada USB enquanto estiver escrevendo o arquivo."

#: ../ui/dwindowui.cpp:149
msgctxt "DWindowUI|"
msgid "Start"
msgstr "Iniciar"

#: ../ui/dwindowui.cpp:257
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#ffffff; font-weight:blod; font-size:18px'>Made "
"successfully</p>"
msgstr "<p style='color:#ffffff; font-weight:blod; font-size:18px'>Feito com sucesso</p>"

#: ../ui/dwindowui.cpp:258
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#b4b4b4; font-size:12px'>Please select USB drive to boot "
"when installing Deepin OS.</p>"
msgstr "<p style='color:#b4b4b4; font-size:12px'>Por favor selecione unidade USB para arrancar quando instalar o Deepin OS.</p>"

#: ../ui/dwindowui.cpp:262
msgctxt "DWindowUI|"
msgid ""
"<a style='color:#b4b4b4; font-size:12px'>If failed, please try to disable "
"EFI option.</a>"
msgstr "<a style='color:#b4b4b4; font-size:12px'>Se falhou, por favor tente desativar a opção EFI.</a>"

