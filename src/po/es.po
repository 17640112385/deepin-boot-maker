#: ../bootmaker.cpp:53 ../bootmaker.cpp:54
msgctxt "BootMaker|"
msgid "Please select an iso image"
msgstr "Por favor seleccione una imagen iso"

#: ../main.cpp:256
msgctxt "QApplication|"
msgid "Deepin Boot Maker"
msgstr "Creador de Arranque Deepin"

#: ../qml/MainUI/UsbCreatorFrame.qml:159
msgctxt "UsbCreatorFrame|"
msgid "<font color='#ffffff'>Easy to use without redundancy</font></br>"
msgstr "<font color='#ffffff'>Fácil para usar sin redundancia</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:166
msgctxt "UsbCreatorFrame|"
msgid ""
"<br><font color='#a7a7a7'>Welcome to use Deepin Boot Maker software and you "
"can quickly create Deepin OS Startup Disk through a simple setting, which "
"supports dual BIOS and <font color='#ebab4c'>UEFI</font> start.</font></br>"
msgstr "<br><font color='#a7a7a7'>Sea bienvenido a usar el programa Creador de Arranque Deepin y podrás crear rápidamente Discos de Inicio de Deepin OS a tráves de una simple configuración, la cual soporta BIOS dual e inicio <font color='#ebab4c'>UEFI</font></font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:188
msgctxt "UsbCreatorFrame|"
msgid "<br><font color='#ffffff'>Select the ISO File:</font></br>"
msgstr "<br><font color='#ffffff'>Seleccione la Imagen ISO:</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:238
msgctxt "UsbCreatorFrame|"
msgid "<br><font color='#ffffff'>Select the USB Flash Drive:</font></br>"
msgstr "<br><font color='#ffffff'>Seleccione el disco USB Flash</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:318
msgctxt "UsbCreatorFrame|"
msgid "<font color='#ffffff'>Support BIOS. Unselect here. </font>"
msgstr "<font color='#ffffff'>Soporte BIOS. Deseleccione aquí. </font>"

#: ../qml/MainUI/UsbCreatorFrame.qml:329
msgctxt "UsbCreatorFrame|"
msgid "Start"
msgstr "Iniciar"

#: ../qml/MainUI/UsbCreatorFrame.qml:369
msgctxt "UsbCreatorFrame|"
msgid ""
"<font color='#ffffff'><br>Please <font color='#ebab4c'>DO NOT</font> remove "
"the USB flash drive or shutdown while file is writing.</br></font>"
msgstr "<font color='#ffffff'><br>Por favor <font color='#ebab4c'>NO</font> remueva el disco USB flash o apague la pc mientras se esta escribiendo el archivo.</br></font>"

#: ../qml/MainUI/UsbCreatorFrame.qml:404
msgctxt "UsbCreatorFrame|"
msgid ""
"<br><font color='#057aff'>Congratulations!</font></br><br><font "
"color='#ffffff'>Deepin OS Startup Disk creates successfully.</font></br>"
msgstr "<br><font color='#057aff'>Felicitaciones!</font></br><br><font color='#ffffff'> el Disco de Incio Deepin OS  fue creado satisfactoriamente.</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:415
msgctxt "UsbCreatorFrame|"
msgid "Quit"
msgstr "Salir"

#: ../qml/MainUI/UsbCreatorFrame.qml:424
msgctxt "UsbCreatorFrame|"
msgid "Reboot"
msgstr "Reinicio"

