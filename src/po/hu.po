msgstr "Indítás"

#: ../ui/dwindowui.cpp:246
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#ffffff; font-weight:blod; font-size:18px'>Made "
"successfully</p>"
msgstr "<p style='color:#ffffff; font-weight:blod; font-size:18px'>Sikeresen elkészült</p>"

#: ../ui/dwindowui.cpp:247
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#b4b4b4; font-size:12px'>Please select USB drive to boot "
"when installing Deepin OS.</p>"
msgstr "<p style='color:#b4b4b4; font-size:12px'>A Deepin telepítéséhez bootoláskor válassza az USB meghajtót</p>"

#: ../ui/dwindowui.cpp:251
msgctxt "DWindowUI|"
msgid ""
"<a style='color:#b4b4b4; font-size:12px'>If failed, please try to disable "
"EFI option.</a>"
msgstr "<a style='color:#b4b4b4; font-size:12px'>Amennyiben sikertelen, próbáld meg letiltani az EFI opciót</a>"

