#: ../unetbootin.cpp:425
msgctxt "unetbootin|"
msgid ""
"No USB flash drives were found. If you have already inserted a USB drive, "
"try reformatting it as FAT32."
msgstr "Nem találtam usb-t. Ha már csatlakoztatott egy usb-t akkor formázza át FAT32-re."

#: ../unetbootin.cpp:444
#, qt-format
msgctxt "unetbootin|"
msgid "%1 not mounted"
msgstr "%1 nincs felcsatolva"

#: ../unetbootin.cpp:445
#, qt-format
msgctxt "unetbootin|"
msgid ""
"You must first mount the USB drive %1 to a mountpoint. Most distributions "
"will do this automatically after you remove and reinsert the USB drive."
msgstr "Csatolnia kell az usb-t a %1 csatolási ponthoz. A legtöbb disztribúció ezt automatikus elvégzi miután eltávolította az usb szöszke és újra csatlakoztatta ."

#: ../unetbootin.cpp:460
msgctxt "unetbootin|"
msgid "Select a disk image file"
msgstr "Válaszon ki egy képfájlt."

#: ../unetbootin.cpp:461
msgctxt "unetbootin|"
msgid "You must select a disk image file to load."
msgstr "Egy képfájlt kell kiválasztani a betöltéshez."

