msgstr "<p style='color:white; font-size:12px;'>%1% befejeződött</p>"

#: ../ui/dwindowui.cpp:127
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#b4b4b4; font-size:12px;'>The making success rate can be "
"improved by formatting USB drive.</p>"
msgstr "<p style='color:#b4b4b4; font-size:12px;'>Az elkészítés sikerességének esélye növelhető az USB meghajtó formázásával.</p>"

#: ../ui/dwindowui.cpp:137
msgctxt "DWindowUI|"
msgid ""
"<a style='color:#b4b4b4; font-size:12px'>Please DO NOT remove the USB drive "
"or shutdown while file is writing.<a>"
msgstr "<a style='color:#b4b4b4; font-size:12px'>Kérem NE TÁVOLÍTSA EL az USB meghajtót az írás folyamata alatt.<a>"

