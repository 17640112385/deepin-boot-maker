#, qt-format
msgctxt "DWindowUI|"
msgid "<p style='color:white; font-size:12px;'>%1% has been completed</p>"
msgstr "<p style='color:white; font-size:12px;'>%1% terminé</p>"

#: ../ui/dwindowui.cpp:126
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#b4b4b4; font-size:12px;'>The making success rate can be "
"improved by formatting USB drive.</p>"
msgstr "<p style='color:#b4b4b4; font-size:12px;'>Les chances de réussite peuvent être améliorées en formatant le lecteur USB</p>"

#: ../ui/dwindowui.cpp:136
msgctxt "DWindowUI|"
msgid ""
"<a style='color:#b4b4b4; font-size:12px'>Please DO NOT remove the USB drive "
"or shutdown while file is writing.<a>"
msgstr "<a style='color:#b4b4b4; font-size:12px'>Ne PAS retirer le lecteur USB ou éteindre le système lors de l'écriture du fichier.<a>"

#: ../ui/dwindowui.cpp:146
msgctxt "DWindowUI|"
msgid "Start"
msgstr "Démarrer"

#: ../ui/dwindowui.cpp:245
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#ffffff; font-weight:blod; font-size:18px'>Made "
"successfully</p>"
msgstr "<p style='color:#ffffff; font-weight:blod; font-size:18px'>Créé avec succès</p>"

#: ../ui/dwindowui.cpp:246
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#b4b4b4; font-size:12px'>Please select USB drive to boot "
"when installing Deepin OS.</p>"
msgstr "<p style='color:#b4b4b4; font-size:12px'>Veuillez sélectionner le lecteur USB à démarrer lors de l'installation de Deepin OS.</p>"

#: ../ui/dwindowui.cpp:250
msgctxt "DWindowUI|"
msgid ""
"<a style='color:#b4b4b4; font-size:12px'>If failed, please try to disable "
"EFI option.</a>"
msgstr "<a style='color:#b4b4b4; font-size:12px'>En cas d'échec, essayez de désactiver l'option EFI.</a>"

