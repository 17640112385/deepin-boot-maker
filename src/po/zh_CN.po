#: ../bootmaker.cpp:71 ../bootmaker.cpp:72
msgctxt "BootMaker|"
msgid "Please select an iso image"
msgstr "请选择ISO文件"

#: ../main.cpp:278
msgctxt "QApplication|"
msgid "Deepin Boot Maker"
msgstr "深度启动盘制作工具"

#: ../qml/MainUI/UsbCreatorFrame.qml:170
msgctxt "UsbCreatorFrame|"
msgid "<font color='#ffffff'>Easy to use without redundancy</font></br>"
msgstr "<font color='#ffffff'>告别繁琐，简单易用</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:177
msgctxt "UsbCreatorFrame|"
msgid ""
"<br><font color='#a7a7a7'>Welcome to use Deepin Boot Maker software and you "
"can quickly create Deepin OS Startup Disk through a simple setting, which "
"supports dual BIOS and <font color='#ebab4c'>UEFI</font> start.</font></br>"
msgstr ""
"<br><font color='#a7a7a7'>欢迎使用深度启动盘软件，通过简单设置即可快速制作Deepin系统启动盘，并且支持BIOS和<font "
"color='#ebab4c'>UEFI</font>双启动。</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:199
msgctxt "UsbCreatorFrame|"
msgid "<br><font color='#ffffff'>Select the ISO File:</font></br>"
msgstr "<br><font color='#ffffff'>选择ISO文件:</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:249
msgctxt "UsbCreatorFrame|"
msgid "<br><font color='#ffffff'>Select the USB Flash Drive:</font></br>"
msgstr "<br><font color='#ffffff'>选择U盘:</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:329
msgctxt "UsbCreatorFrame|"
msgid "<font color='#ffffff'>Support BIOS. Unselect here. </font>"
msgstr "<font color='#ffffff'>旧主板BIOS支持， 默认请不要勾选。</font>"

#: ../qml/MainUI/UsbCreatorFrame.qml:338
msgctxt "UsbCreatorFrame|"
msgid "<font color='#ffffff'>Format USB disk.</font>"
msgstr "<font color='#ffffff'>安装前格式化U盘，提高制作成功率。</font>"

#: ../qml/MainUI/UsbCreatorFrame.qml:344
msgctxt "UsbCreatorFrame|"
msgid "Format USB Disk"
msgstr "格式化U盘"

#: ../qml/MainUI/UsbCreatorFrame.qml:345
msgctxt "UsbCreatorFrame|"
msgid ""
"Would you want to FORMAT the USB disk? All partitions on usb disk will be "
"deleted, please backup you data first!"
msgstr "格式化U盘将丢失U盘上所有资料，请做好备份后点确定按钮"

#: ../qml/MainUI/UsbCreatorFrame.qml:369
msgctxt "UsbCreatorFrame|"
msgid "Start"
msgstr "开始创建"

#: ../qml/MainUI/UsbCreatorFrame.qml:411
msgctxt "UsbCreatorFrame|"
msgid ""
"<font color='#ffffff'><br>Please <font color='#ebab4c'>DO NOT</font> remove "
"the USB flash drive or shutdown while file is writing.</br></font>"
msgstr ""
"<font color='#ffffff'><br>正在写入文件，<font color='#ebab4c'>请不要</font> "
"移除U盘或者关机！</br></font>"

#: ../qml/MainUI/UsbCreatorFrame.qml:447
msgctxt "UsbCreatorFrame|"
msgid ""
"<br><font color='#057aff'>Congratulations!</font></br><br><font "
"color='#ffffff'>Deepin OS Startup Disk creates successfully.</font></br>"
msgstr ""
"<br><font color='#057aff'>恭喜！</font></br><br><font "
"color='#ffffff'>Deepin系统启动盘制作成功。</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:458
msgctxt "UsbCreatorFrame|"
msgid "Quit"
msgstr "完成"

#: ../qml/MainUI/UsbCreatorFrame.qml:467
msgctxt "UsbCreatorFrame|"
msgid "Reboot"
msgstr "重启"

