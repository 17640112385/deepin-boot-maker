msgstr "<p style='color:white; font-size:12px;'>%1% completato</p>"

#: ../ui/dwindowui.cpp:126
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#b4b4b4; font-size:12px;'>The making success rate can be "
"improved by formatting USB drive.</p>"
msgstr "<p style='color:#b4b4b4; font-size:12px;'>il grado di successo potrebbe aumentare formattando il dispositivo.</p>"

#: ../ui/dwindowui.cpp:136
msgctxt "DWindowUI|"
msgid ""
"<a style='color:#b4b4b4; font-size:12px'>Please DO NOT remove the USB drive "
"or shutdown while file is writing.<a>"
msgstr "<a style='color:#b4b4b4; font-size:12px'>Non rimuovere il dispositivo USB mentre sto elaborando.<a>"

#: ../ui/dwindowui.cpp:146
msgctxt "DWindowUI|"
msgid "Start"
msgstr "Inizio"

#: ../ui/dwindowui.cpp:245
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#ffffff; font-weight:blod; font-size:18px'>Made "
"successfully</p>"
msgstr "<p style='color:#ffffff; font-weight:blod; font-size:18px'>Operazione conclusa con successo!</p>"

#: ../ui/dwindowui.cpp:246
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#b4b4b4; font-size:12px'>Please select USB drive to boot "
"when installing Deepin OS.</p>"
msgstr "<p style='color:#b4b4b4; font-size:12px'>Ricordati di selezionare il dispositivo USB durante il boot per poi installare Deepin OS.</p>"

#: ../ui/dwindowui.cpp:250
msgctxt "DWindowUI|"
msgid ""
"<a style='color:#b4b4b4; font-size:12px'>If failed, please try to disable "
"EFI option.</a>"
msgstr "<a style='color:#b4b4b4; font-size:12px'>Se fallisce disabilita l'opzione EFI.</a>"

