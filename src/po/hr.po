#: ../bootmaker.cpp:53 ../bootmaker.cpp:54
msgctxt "BootMaker|"
msgid "Please select an iso image"
msgstr "Odaberi iso datoteku"

#: ../main.cpp:256
msgctxt "QApplication|"
msgid "Deepin Boot Maker"
msgstr "Deepin stvaratelj diska pokretanja"

#: ../qml/MainUI/UsbCreatorFrame.qml:159
msgctxt "UsbCreatorFrame|"
msgid "<font color='#ffffff'>Easy to use without redundancy</font></br>"
msgstr "<font color='#ffffff'>Easy to use without redundancy</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:166
msgctxt "UsbCreatorFrame|"
msgid ""
"<br><font color='#a7a7a7'>Welcome to Deepin Boot Maker. After setting a few "
"options, you'll be able to create a Deepin OS Startup Disk, which supports "
"both BIOS and <font color='#ebab4c'>UEFI</font> boot.</font></br>"
msgstr "<br><font color='#a7a7a7'>Dobrodošli u Deepin stvaratelja diska pokretanja. Nakon postavljanja nekoliko mogućnosti, moći ćete stvoriti Deepin OS Disk pokretanja, koji podržava BIOS i <font color='#ebab4c'>UEFI</font> pokretanje sustava.</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:188
msgctxt "UsbCreatorFrame|"
msgid "<br><font color='#ffffff'>Select the ISO File:</font></br>"
msgstr "<br><font color='#ffffff'>Odaberi ISO datoteku:</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:238
msgctxt "UsbCreatorFrame|"
msgid "<br><font color='#ffffff'>Select the USB Flash Drive:</font></br>"
msgstr "<br><font color='#ffffff'>Odaberi USB disk:</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:318
msgctxt "UsbCreatorFrame|"
msgid "<font color='#ffffff'>Support BIOS. Unselect here. </font>"
msgstr "<font color='#ffffff'>Podržani BIOS. Odznačite ovdje. </font>"

#: ../qml/MainUI/UsbCreatorFrame.qml:329
msgctxt "UsbCreatorFrame|"
msgid "Start"
msgstr "Pokreni"

#: ../qml/MainUI/UsbCreatorFrame.qml:369
msgctxt "UsbCreatorFrame|"
msgid ""
"<font color='#ffffff'><br>Please <font color='#ebab4c'>DO NOT</font> remove "
"the USB flash drive or shutdown while file is writing.</br></font>"
msgstr "<font color='#ffffff'><br>Molim <font color='#ebab4c'>NEMOJTE</font> ukloniti USB disk ili isključiti računalo tijekom zapisivanja.</br></font>"

#: ../qml/MainUI/UsbCreatorFrame.qml:404
msgctxt "UsbCreatorFrame|"
msgid ""
"<br><font color='#057aff'>Congratulations!</font></br><br><font "
"color='#ffffff'>Deepin OS Startup Disk is created successfully.</font></br>"
msgstr "<br><font color='#057aff'>Čestitke!</font></br><br><font color='#ffffff'>Deepin OS Disk pokretanja je stvoren uspješno.</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:415
msgctxt "UsbCreatorFrame|"
msgid "Quit"
msgstr "Zatvori"

#: ../qml/MainUI/UsbCreatorFrame.qml:424
msgctxt "UsbCreatorFrame|"
msgid "Reboot"
msgstr "Ponovno pokreni"

#: ../unetbootin.cpp:252 ../unetbootin.cpp:258 ../unetbootin.cpp:262
#: ../unetbootin.cpp:266 ../unetbootin.cpp:270 ../unetbootin.cpp:276
#: ../unetbootin.cpp:304
msgctxt "unetbootin|"
msgid "either"
msgstr "ili"

#: ../unetbootin.cpp:283
msgctxt "unetbootin|"
msgid "LiveUSB persistence"
msgstr "LiveUSB dodatni prostor pohrane"

#: ../unetbootin.cpp:298
msgctxt "unetbootin|"
msgid "FAT32-formatted USB drive"
msgstr "FAT32-formatiran USB disk"

#: ../unetbootin.cpp:302
msgctxt "unetbootin|"
msgid "EXT2-formatted USB drive"
msgstr "EXT2-formatiran USB disk"

#: ../unetbootin.cpp:424
msgctxt "unetbootin|"
msgid "Insert a USB flash drive"
msgstr "Priključite USB disk"

#: ../unetbootin.cpp:425
msgctxt "unetbootin|"
msgid ""
"No USB flash drives were found. If you have already inserted a USB drive, "
"try reformatting it as FAT32."
msgstr "Nema pronađenih USB diskova. Ako ste već priključili USB disk, pokušajte ga ponovno formatirati kao FAT32."

#: ../unetbootin.cpp:444
#, qt-format
msgctxt "unetbootin|"
msgid "%1 not mounted"
msgstr "%1 nije montiran"

#: ../unetbootin.cpp:445
#, qt-format
msgctxt "unetbootin|"
msgid ""
"You must first mount the USB drive %1 to a mountpoint. Most distributions "
"will do this automatically after you remove and reinsert the USB drive."
msgstr "Prvo morate montirati USB disk %1 u točku montiranja. Većina distribucija će to učiniti automatski nakon što uklonite i ponovno priključite USB disk."

#: ../unetbootin.cpp:460
msgctxt "unetbootin|"
msgid "Select a disk image file"
msgstr "Odaberi datoteku slike diska"

#: ../unetbootin.cpp:461
msgctxt "unetbootin|"
msgid "You must select a disk image file to load."
msgstr "Morate odabrati datoteku slike diska za učitavanje."

#: ../unetbootin.cpp:475
msgctxt "unetbootin|"
msgid "Diskimage file not found"
msgstr "Slika diska nije pronađena."

#: ../unetbootin.cpp:476
#, qt-format
msgctxt "unetbootin|"
msgid "The specified diskimage file %1 does not exist."
msgstr "Određena datoteka slike diska %1 ne postoji."

#: ../unetbootin.cpp:588
#, qt-format
msgctxt "unetbootin|"
msgid "%1 exists, overwrite?"
msgstr "%1 postoji, želite li prepisati?"

#: ../unetbootin.cpp:589
#, qt-format
msgctxt "unetbootin|"
msgid ""
"The file %1 already exists. Press 'Yes to All' to overwrite it and not be "
"prompted again, 'Yes' to overwrite files on an individual basis, and 'No' to"
" retain your existing version. If in doubt, press 'Yes to All'."
msgstr "Datoteka %1 već postoji. Pritisnite 'Da za sve' za prepisivanje i bez ponovnog upita, 'Da' za prepisivanje datoteka na pojedinačnoj osnovi i 'Ne' za zadržavanje postojeće inačice. Ako ste u dvojbi, pritisnite 'Da za sve'."

#: ../unetbootin.cpp:615
#, qt-format
msgctxt "unetbootin|"
msgid "%1 is out of space, abort installation?"
msgstr "%1 je nestalo slobodnog prostora, prekini instalaciju?"

#: ../unetbootin.cpp:616
#, qt-format
msgctxt "unetbootin|"
msgid ""
"The directory %1 is out of space. Press 'Yes' to abort installation, 'No' to"
" ignore this error and attempt to continue installation, and 'No to All' to "
"ignore all out-of-space errors."
msgstr "Direktorij %1 nema slobodnog prostora. Pritisnite 'Da' za prekid instalacije, 'Ne' za zanemarivanje ove greške i pokušaj nastavka instalacije i 'Ne za sve' za zanemarivanje svih greški nedostatka slobodnog prostora."

