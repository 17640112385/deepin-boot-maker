#: ../bootmaker.cpp:53 ../bootmaker.cpp:54
msgctxt "BootMaker|"
msgid "Please select an iso image"
msgstr "Selecteer een ISO-schijfafbeelding"

#: ../main.cpp:256
msgctxt "QApplication|"
msgid "Deepin Boot Maker"
msgstr "Deepin Opstartmedium-maker"

#: ../qml/MainUI/UsbCreatorFrame.qml:159
msgctxt "UsbCreatorFrame|"
msgid "<font color='#ffffff'>Easy to use without redundancy</font></br>"
msgstr "<font color='#ffffff'>Makkelijk te gebruiken zonder ingewikkeldheid</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:166
msgctxt "UsbCreatorFrame|"
msgid ""
"<br><font color='#a7a7a7'>Welcome to Deepin Boot Maker. After setting a few "
"options, you'll be able to create a Deepin OS Startup Disk, which supports "
"both BIOS and <font color='#ebab4c'>UEFI</font> boot.</font></br>"
msgstr "<br><font color='#a7a7a7'>Welkom bij Deepin Opstartmedium-maker. Na het instellen van een aantal opties kunt u een Deepin OS-opstartschijf maken welke zowel BIOS als <font color='#ebab4c'>UEFI</font> ondersteunt.</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:188
msgctxt "UsbCreatorFrame|"
msgid "<br><font color='#ffffff'>Select the ISO File:</font></br>"
msgstr "<br><font color='#ffffff'>Selecteer het ISO-bestand:</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:238
msgctxt "UsbCreatorFrame|"
msgid "<br><font color='#ffffff'>Select the USB Flash Drive:</font></br>"
msgstr "<br><font color='#ffffff'>Selecteer de USB-schijf:</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:318
msgctxt "UsbCreatorFrame|"
msgid "<font color='#ffffff'>Support BIOS. Unselect here. </font>"
msgstr "<font color='#ffffff'>BIOS ondersteunen. Deselecteer hier. </font>"

#: ../qml/MainUI/UsbCreatorFrame.qml:329
msgctxt "UsbCreatorFrame|"
msgid "Start"
msgstr "Beginnen"

#: ../qml/MainUI/UsbCreatorFrame.qml:369
msgctxt "UsbCreatorFrame|"
msgid ""
"<font color='#ffffff'><br>Please <font color='#ebab4c'>DO NOT</font> remove "
"the USB flash drive or shutdown while file is writing.</br></font>"
msgstr "<font color='#ffffff'><br>Verwijder de USB-schijf of sluiter de computer <font color='#ebab4c'>NIET</font> tijdens het schrijven.</br></font>"

#: ../qml/MainUI/UsbCreatorFrame.qml:404
msgctxt "UsbCreatorFrame|"
msgid ""
"<br><font color='#057aff'>Congratulations!</font></br><br><font "
"color='#ffffff'>Deepin OS Startup Disk is created successfully.</font></br>"
msgstr "<br><font color='#057aff'>Gefeliciteerd!</font></br><br><font color='#ffffff'>Deepin OS-opstartschijf is succesvol gecreërd!</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:415
msgctxt "UsbCreatorFrame|"
msgid "Quit"
msgstr "Afsluiten"

#: ../qml/MainUI/UsbCreatorFrame.qml:424
msgctxt "UsbCreatorFrame|"
msgid "Reboot"
msgstr "Herstarten"

