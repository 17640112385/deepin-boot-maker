#: ../bootmaker.cpp:71 ../bootmaker.cpp:72
msgctxt "BootMaker|"
msgid "Please select an iso image"
msgstr "Por favor selecione uma imagem de disco"

#: ../main.cpp:278
msgctxt "QApplication|"
msgid "Deepin Boot Maker"
msgstr "Deepin Boot Maker"

#: ../qml/MainUI/UsbCreatorFrame.qml:163
msgctxt "UsbCreatorFrame|"
msgid "<font color='#ffffff'>Easy to use without redundancy</font></br>"
msgstr "<font color='#ffffff'>Fácil de usar sem redundância</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:170
msgctxt "UsbCreatorFrame|"
msgid ""
"<br><font color='#a7a7a7'>Welcome to use Deepin Boot Maker software and you "
"can quickly create Deepin OS Startup Disk through a simple setting, which "
"supports dual BIOS and <font color='#ebab4c'>UEFI</font> start.</font></br>"
msgstr ""
"<br><font color='#a7a7a7'>Bem vindo ao aplicativo Deepin Boot Maker e você "
"pode rapidamente criar um Disco de Inicialização Deepin OS através de "
"simples ajustes, que suporta dual Bios e <font color='#ebab4c'>UEFI</font> "
"start.</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:192
msgctxt "UsbCreatorFrame|"
msgid "<br><font color='#ffffff'>Select the ISO File:</font></br>"
msgstr "<br><font color='#ffffff'>Selecione o arquivo ISO:</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:242
msgctxt "UsbCreatorFrame|"
msgid "<br><font color='#ffffff'>Select the USB Flash Drive:</font></br>"
msgstr "<br><font color='#ffffff'>Selecione o Disco Removível USB:</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:265
msgctxt "UsbCreatorFrame|"
msgid ""
"<font color='#ffffff'>Format USB flash disk before installation to improve "
"the making success rate.</font>"
msgstr ""

#: ../qml/MainUI/UsbCreatorFrame.qml:271
msgctxt "UsbCreatorFrame|"
msgid "Format USB flash disk"
msgstr ""

#: ../qml/MainUI/UsbCreatorFrame.qml:272
msgctxt "UsbCreatorFrame|"
msgid ""
"All data will be lost during formatting, please back up in advance and then "
"press OK button."
msgstr ""

#: ../qml/MainUI/UsbCreatorFrame.qml:296
msgctxt "UsbCreatorFrame|"
msgid "<font color='#ffffff'>Support BIOS. Unselect here. </font>"
msgstr "<font color='#ffffff'>BIOS Suporte. Deselecione aqui. </font>"

#: ../qml/MainUI/UsbCreatorFrame.qml:307
msgctxt "UsbCreatorFrame|"
msgid "Start"
msgstr "Iniciar"

#: ../qml/MainUI/UsbCreatorFrame.qml:349
msgctxt "UsbCreatorFrame|"
msgid ""
"<font color='#ffffff'><br>Please <font color='#ebab4c'>DO NOT</font> remove "
"the USB flash drive or shutdown while file is writing.</br></font>"
msgstr ""
"<font color='#ffffff'><br>Por favor <font color='#ebab4c'>NÃO</font> remova "
"o disco removível USB ou desligue enquanto está sendo copiado.</br></font>"

#: ../qml/MainUI/UsbCreatorFrame.qml:385
msgctxt "UsbCreatorFrame|"
msgid ""
"<br><font color='#057aff'>Congratulations!</font></br><br><font "
"color='#ffffff'>Deepin OS Startup Disk creates successfully.</font></br>"
msgstr ""
"<br><font color='#057aff'>Parabéns!</font></br><br><font color='#ffffff'>A "
"criação do Disco de inicialização Deepin OS foi um sucesso.</font></br>"

#: ../qml/MainUI/UsbCreatorFrame.qml:396
msgctxt "UsbCreatorFrame|"
msgid "Quit"
msgstr "Sair"

#: ../qml/MainUI/UsbCreatorFrame.qml:405
msgctxt "UsbCreatorFrame|"
msgid "Reboot"
msgstr "Reiniciar"

