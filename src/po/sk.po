msgctxt "BootMaker|"
msgid "Disk image file not found"
msgstr "Obraz disku nebol nájdený"

#: ../bootmaker.cpp:205
#, qt-format
msgctxt "BootMaker|"
msgid "The specified disk image file %1 does not exist."
msgstr "Daný súbor s obrazom disku %1 neexistuje. "

