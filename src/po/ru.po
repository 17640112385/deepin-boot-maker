#, qt-format
msgctxt "DWindowUI|"
msgid "<p style='color:white; font-size:12px;'>%1% has been completed</p>"
msgstr "<p style='color:white; font-size:12px;'>%1% завершено</p>"

#: ../ui/dwindowui.cpp:127
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#b4b4b4; font-size:12px;'>The making success rate can be "
"improved by formatting USB drive.</p>"
msgstr "<p style='color:#b4b4b4; font-size:12px;'>Вероятость успеха, можно повысить, путём форматирования USB носителя.</p>"

#: ../ui/dwindowui.cpp:137
msgctxt "DWindowUI|"
msgid ""
"<a style='color:#b4b4b4; font-size:12px'>Please DO NOT remove the USB drive "
"or shutdown while file is writing.<a>"
msgstr "<a style='color:#b4b4b4; font-size:12px'>НЕ извлекайте USB-носитель и  не отключайте компьютер во времязаписи файла.<a>"

#: ../ui/dwindowui.cpp:147
msgctxt "DWindowUI|"
msgid "Start"
msgstr "Старт"

#: ../ui/dwindowui.cpp:246
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#ffffff; font-weight:blod; font-size:18px'>Made "
"successfully</p>"
msgstr "<p style='color:#ffffff; font-weight:blod; font-size:18px'>Успешно выполнено</p>"

#: ../ui/dwindowui.cpp:247
msgctxt "DWindowUI|"
msgid ""
"<p style='color:#b4b4b4; font-size:12px'>Please select USB drive to boot "
"when installing Deepin OS.</p>"
msgstr "<p style='color:#b4b4b4; font-size:12px'>Выбирите USB-носитель для загрузки, при установке Deepin ОС.</p>"

#: ../ui/dwindowui.cpp:251
msgctxt "DWindowUI|"
msgid ""
"<a style='color:#b4b4b4; font-size:12px'>If failed, please try to disable "
"EFI option.</a>"
msgstr "<a style='color:#b4b4b4; font-size:12px'>При неудаче, попробуйте отключить опцию EFI.</a>"

